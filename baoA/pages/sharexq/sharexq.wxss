/* 弹出框 */
.tanbox{
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 100;
  left: 0;
  top: 0;
}
.tanbox .black{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  z-index: 101;
  left: 0;
  top: 0;
  position: absolute;
}
.tanbox .conbox{
  width: 100%;
  position: absolute;
  height: 700rpx;
  z-index: 110;
  left: 0;
  bottom: -700rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
}
.conboxshow {
  transform: translateY(-700rpx);
  transition: all 0.5s;
}

.conboxhidden {
  transform: translateY(700rpx);
  transition: all 0.7s;
}
.blackshow {
  opacity: 1;
  transition: all 0.5s;
}

.blackhidden {
  opacity: 0;
  transition: all 0.7s;
}
.tanbox .conbox .tanboxheader{
  width: 100%;
  height: 200rpx;
  box-sizing: border-box;
  padding: 0 15rpx;
  display: flex;
  justify-content: flex-start;
}
.tanbox .conbox .tanboxheader image{
  width: 208rpx;
  height: 208rpx;
  border-radius: 20rpx;
  position: relative;
  top: -20rpx;
  margin-right: 20rpx;
}
.tanbox .conbox .tanboxheader .titbox{
  flex: 1;
  height:100%;
  margin-top: 25rpx;

}
.tanbox .conbox .tanboxheader .titbox .tit{
  font-size: 32rpx;
  color: #333;
  line-height: 44rpx;
  width: 100%;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  margin-bottom: 30rpx;
}
.tanbox .conbox .tanboxheader .titbox .con{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.tanbox .conbox .tanboxheader .titbox view{
  font-size: 34rpx;
  color: #EE1E3E;
}
.tanbox .conbox .tanboxheader .titbox text{
  font-size: 24rpx;
  color: #333;
}
.tanbox .conbox .guigetit{
  width: 100%;
  height: 80rpx;
  box-sizing: border-box;
  padding: 0 15rpx;
  line-height: 80rpx;
  color: #333;
  font-size: 28rpx;
}
.tanbox .conbox .guigecon{
  width: 100%;
  height: 160rpx;
  overflow-y: auto;
  box-sizing: border-box;
  padding: 0 40rpx;
  padding-bottom: 20rpx;
  padding-top: 20rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.tanbox .conbox .guigecon .list{
  font-size: 26rpx;
  color: #333;
  padding: 11rpx 24rpx;
