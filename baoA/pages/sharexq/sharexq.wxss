/* pages/pintuanxq/pintuanxq.wxss */
@import "../../../utils/public.wxss";/*引用图标*/
.Pintuanxq{
  box-sizing: border-box;
  padding-top: 100rpx;
  padding-bottom: 100rpx;
  overflow-y: auto;
}
.canvasimg {
  width: 500rpx;
  height: 400rpx;
}
/* 头部 */
.header{
  width: 100%;
  height: 100rpx;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 190rpx;
  align-items: center;
  z-index: 10;
  box-shadow: 0 0 5px 2px rgba(144, 144, 144, 0.35);
}
.header .list{
  font-size: 32rpx;
  color: #333;
  height: 100rpx;
  line-height: 100rpx;
  box-sizing: border-box;
}
.header .list.active{
  color: #E31718;
  border-bottom: 4rpx solid #E31718;
}
.header image{
  position: absolute;
  right: 30rpx;
  top: 32rpx;
  width: 42rpx;
  height: 44rpx;
}
/* 轮播图 */
.banner{
  width: 750rpx;
  height: 750rpx;
  
}
.banner image{
  width: 100%;
  height: 100%;
}
/* 商品信息 */ 
.shopbox{
  width: 100%;
  height: 214rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column; 
  padding: 0 30rpx;
  background: #fff;
  overflow: hidden;
}
.shopbox .tit{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.shopbox .tit .titleft{
  display: flex;
  justify-content: flex-start;
  align-items: baseline;
}
.shopbox .tit .titleft .qian{
  font-size: 24rpx;
  color: #EC1D3B;
}
.shopbox .tit .titleft .money{
  font-size: 48rpx;
  color: #EC1D3B;
}
.shopbox .tit .titleft .danwei{
  color: #999;
  font-size: 22rpx;
}
.shopbox .tit .titleft .danmai{
  color: #999;
  font-size: 22rpx;
  margin-left: 25rpx;

}
.shopbox .tit .titright{
  display: flex;
  justify-content: space-between;

}
.shopbox .tit .titright .list{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 80rpx;
}
.shopbox .tit .titright .list.fenxiangbox{
  padding: 0;
  background: none;
  border: none;
  line-height: 1;
  overflow: visible;
}
.shopbox .tit .titright .list.fenxiangbox::after{
  border: none;
}

.shopbox .tit .titright .list image{
  width: 36rpx;
  height: 36rpx;
  margin-bottom: 15rpx;
}
.shopbox .tit .titright .list text{
  font-size: 20rpx;
  color: #333;
}
.shopbox .con{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  box-sizing: border-box;
  line-height: 42rpx;
  min-height: 48rpx;
}
.shopbox .con .biaoqian{
  padding: 4rpx 7rpx;
  border: 1rpx solid #E9130D;
  border-radius: 10rpx;
  color: #E9130D;
  font-size: 24rpx;
  margin-right: 10rpx;
  transform: rotateZ(360deg);
}
.shopbox .con .name{
  color: #333;
  letter-spacing: 2rpx;
  font-size:28rpx;
  line-height: 1;
}
/* 店铺信息 */
.dianbox{
  width: 100%;
  height: 212rpx;
  background: #fff;
  margin-top: 10rpx;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
}
.dianbox .top{
  display: flex;
  justify-content: flex-start;
  height: 120rpx;
  width: 100%;
}
.dianbox .top image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.dianbox .top .topright{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.dianbox .top .topright .tr{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.dianbox .top .topright .tit{
  font-size: 30rpx;
  color: #333;
  margin-top: 10rpx;
  margin-bottom: 15rpx;
  width: 330rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.dianbox .top .topright .pingfen{
  font-size: 24rpx;
  color: #999;
}
.dianbox .top .topright .dizhi{
  display: flex;
  align-items: center;
  justify-content: start;

}
.dianbox .top .topright .dizhi image{
  width:27rpx;
  height: 30rpx;
  margin: 0;
  margin-right: 10rpx;
}
.dianbox .top .topright .dizhi view{
  color: #999;
  font-size: 24rpx;
  width: 320rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.dianbox .top .topright .juli{
  color: #999;
  font-size: 24rpx;
}
.dianbox .bottom{
  width: 100%;
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;

}
.dianbox .bottom view{
  font-size: 22rpx;
  width: 148rpx;
  height: 40rpx;
  border: 1rpx solid #666;
  border-radius: 40rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 57rpx;
  color: #666;
  transform: rotateZ(360deg);
}
.dianbox .bottom view text{
  font-size: 22rpx;
  color: #666;
  margin-left: 10rpx;
}
.dianbox .bottom view .iconfont{
  font-size: 22rpx;
  color: #666;
}
/* 评价 */
.pingjiabox{
  width: 100%;
  background: #fff;
  margin-top: 10rpx;
  display: flex;
  flex-direction: column;
}
.pingjiabox .pjhead{
