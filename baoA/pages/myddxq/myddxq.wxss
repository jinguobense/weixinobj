/* 弹出框 */
.tanbox{
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 100;
  left: 0;
  top: 0;
}
.tanbox .black{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  z-index: 2000;
  left: 0;
  top: 0;
  position: absolute;
}
.tanbox .conbox{
  width: 450rpx;
  position: absolute;
  z-index: 3000;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  background: #fff;
  display: flex;
  flex-direction: column;
  height: 450rpx;
  align-items: center;
  justify-content: center;
}

.tanbox .conboxshow {
  opacity: 1;
  transition: all 0.3s;
}

.tanbox .conboxhidden {
  opacity: 0;
  transition: all 0.3s;
}
.tanbox .blackshow {
  opacity: 1;
  transition: all 0.3s;
}

.tanbox .blackhidden {
  opacity: 0;
  transition: all 0.3s;
}