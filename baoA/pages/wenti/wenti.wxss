/* pages/wenti/wenti.wxss */
@import "../../../utils/public.wxss";/*引用图标*/
.Wenti{
  width: 100%;
  height: 100%;
  background: #fff;
  box-sizing: border-box;
  padding-left: 30rpx;
  border-top: 15rpx solid #f4f4f4;
}
.list{
  width: 100%;
  border-bottom: 2rpx solid #E7E7E7;
  height:120rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  box-sizing: border-box;
  padding-right: 30rpx;
}
.list image{
  width: 28rpx;
  height: 28rpx;
  margin-right: 20rpx;
}
.list text{
  font-size: 28rpx;
  color: #333;

}
.list .iconfont{
  font-size: 30rpx;
  color: #ccc;
  margin-left: auto;
}