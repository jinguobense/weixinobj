/* pages/kefuhelp/kefuhelp.wxss */
@import "../../../utils/public.wxss";/*引用图标*/
.Kefuhelp{
  width: 100%;
  height: 100%;
  background: #fff;

}
.list{
  width: 100%;
  height: 100rpx;
  border-bottom: 2rpx solid #f5f5f5;
  box-sizing: border-box;
  padding-left: 55rpx;
  padding-right: 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;

}
.list view{
  font-size: 28rpx;
  color: #333;
}
.list .iconfont{
  font-size: 30rpx;
  color: #E4E4E4;
}
.list text{
  font-size: 30rpx;
  color: #159600;
  margin-right: 5rpx;
}