/* pages/myddxq/myddxq.wxss */
.Myddxq{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding-bottom: 90rpx;
  overflow-y: auto;
}
.dizhibox{
  width: 100%;
  height: 192rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  padding: 0 36rpx;
  margin-bottom:10rpx;
  justify-content: center;
}
.dizhibox .top{
  font-size: 32rpx;
  color: #333;
  margin-bottom: 20rpx;
  letter-spacing: 2rpx;
  
}
.dizhibox .bottom{
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.dizhibox .bottom .dizhi{
  width: 21rpx;
  height: 26rpx;
  display: block;
  margin-right: 17rpx;
}
.dizhibox .bottom .zitibox{
  font-size: 26rpx;
  line-height: 40rpx;
  width: 530rpx;
}
.dizhibox .bottom .zitibox .tit{
  color: #666;
}
.dizhibox .bottom .zitibox .con{
  color: #666;
}
.dizhibox .bottom .tel{
  width: 40rpx;
  height: 40rpx;
  display: block;
  margin-left: auto;
  margin-right: 20rpx;
}

/* 购买商品信息 */
.listbox{
  width: 100%;
  background: #fff;
  display: flex;
  flex-direction: column;
  margin-bottom: 10rpx;

}
.listbox .list{
  width: 100%;
  height: 220rpx;
  background: #fff;
  border-bottom: 1rpx solid #f5f5f5;
  box-sizing: border-box;
  padding: 0 36rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.listbox .list image{
  width: 176rpx;
  height: 176rpx;
  border-radius: 10rpx;
  margin-right: 15rpx;
}
.listbox .list .listright{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.listbox .list .listright .tit{
  font-size: 26rpx;
  color: #333;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  width: 100%;
  letter-spacing: 2rpx;
}
.listbox .list .listright .guige{
  font-size: 24rpx;
  color: #666;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  width: 100%;
  margin: 30rpx 0;
}
.listbox .list .listright .bottom{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.listbox .list .listright .bottom .money{
  font-size: 28rpx;
  color: #DC2247;
}
.listbox .list .listright .bottom .num{
  color: #666;
  font-size: 28rpx;
}


/* 订单信息 */
.dingdanbox{
  width: 100%;
  margin-bottom: 8rpx;
  box-sizing: border-box;
  padding: 20rpx 36rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  background: #fff;
}
.dingdanbox .list{
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin: 12rpx 0;
}
.dingdanbox .list text{
  font-size: 26rpx;
  color: #666;
  letter-spacing: 2rpx;
}
.dingdanbox .list view{
  font-size: 22rpx;
  color: #999;
  border: 1rpx solid #999;
  width: 84rpx;
  height: 34rpx;
  text-align: center;
  line-height: 34rpx;
  margin-left: auto;
  border-radius: 34rpx;
}
/* 底部 */
.foot{
  width: 100%;
  height: 90rpx;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  box-sizing: border-box;
  padding: 0 36rpx;
}
.foot .huibox{
  width: 160rpx;
  height: 48rpx;
  border-radius: 48rpx;
  color: #999;
  border: 1rpx solid #999;
  margin-left: 20rpx;
  font-size: 26rpx;
  text-align: center;
  line-height: 48rpx;
  letter-spacing: 2rpx;
}
.foot .redbox{
  width: 160rpx;
  height: 48rpx;
  border-radius: 48rpx;
  color: #DD1D1D;
  border: 1rpx solid #FF0000;
  margin-left: 20rpx;
  font-size: 26rpx;
  text-align: center;
  line-height: 48rpx;
  letter-spacing: 2rpx;
}
.hexiaobox{
  width: 100%;
  height: 80rpx;
  background: #fff;
  box-sizing: border-box;
  padding: 0 36rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 10rpx;
}
.hexiaobox view{
  font-size: 28rpx;
  color: #333;
  letter-spacing: 2rpx;
}
.hexiaobox text{
  font-size: 30rpx;
  color: #00AD1C;
  
}