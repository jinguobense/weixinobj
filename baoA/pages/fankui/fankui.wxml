<!--pages/fankui/fankui.wxml-->
<view class="Fankui">
  <view class="tit">反馈主题</view>
  <input type="text" placeholder="请输入您要反馈的问题，例如：账号问题"     bindinput="titwatch"></input>
  <view class="tit">反馈内容</view>
  <textarea placeholder="请输入具体内容：" placeholder-style="line-height: 40rpx;font-size: 24rpx;" bindinput="conwatch"></textarea>
  <view class="btnbox">
    <view class="btn" catchtap="msg">提交反馈</view>
  </view>
</view>
