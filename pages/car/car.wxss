 /* pages/car/car.wxss */
/* 头部 */
.Car{
  width: 100%;
  box-sizing: border-box;
  padding-top: 80rpx;
}
.header{
  width: 100%;
  height: 80rpx;
  background: #fff;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 100;
}
.header .left{
  display: flex;
  align-items: center;
  margin-left: 26rpx;
  
}
.header .left image{
  width: 32rpx;
  height: 32rpx;
  margin-right: 14rpx;
}
.header .left text{
  color: #666;
  font-size: 28rpx;
}
.header .center{
  margin-left: auto;
  margin-right: 70rpx;
}
.header .center image{
  width: 34rpx;
  height: 34rpx;
  
}
.header .end{
  font-size: 28rpx;
  color: #333;
  margin-right: 50rpx;
}
.guanlibox{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  box-sizing: border-box;
  padding-left: 20rpx;
  height: 100%;
  margin-left: auto;
}
.guanlibox image{
  width: 26rpx;
  height: 26rpx;
  margin-right: 15rpx;
}
.guanlibox view{
  margin-right: 50rpx;
  font-size: 28rpx;
  color: #333;
}
/* 购物车列表块 */
.carbox{
  box-sizing: border-box;
  
}
.carbox .no{
  width: 100%;
  height: 400rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
}
.carbox .no image{
  width: 348rpx;
  margin: 0 auto;
}
.carbox .carlist{
  margin-bottom: 20rpx;
  width: 100%;
  min-height: 400rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
}
.carbox .carlist:first-child{
  margin-top: 20rpx;
}
.carlist .listtop{
  width: 100%;
  height: 82rpx;
  border-bottom: 1rpx solid #f5f5f5;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.carlist .listtop .left{
  margin: 0 26rpx;
  display: flex;
  height: 82rpx;
  align-items: center;
}
.carlist .listtop .left image{
  width: 32rpx;
  height: 32rpx;
}
.carlist .listtop .right{
  display: flex;
  height: 82rpx;
  align-items: center;
  justify-content: flex-start;
}
.carlist .listtop .right image{
  width: 35rpx;
  height: 31rpx;
  margin-right: 10rpx;
}
.carlist .listtop .right text{
  font-size: 30rpx;
  color: #333;
}
.carlist .listcenter{
  width: 100%;
  height: 210rpx;
  border-bottom: 1rpx solid #f5f5f5;
  display: flex;
  justify-content: flex-start;
}
.carlist .listcenter .left{
  margin: 0 26rpx;
  display: flex;
  height: 210rpx;
  align-items: center;
}
.carlist .listcenter .left image{
  width: 32rpx;
  height: 32rpx;
}
.carlist .listcenter .listcon{
  display: flex;
  justify-content: flex-start;
  box-sizing: border-box;
  padding: 22rpx 0;
  
  padding-right: 30rpx;
}
.carlist .listcenter .listcon image{
  width: 166rpx;
  height: 166rpx;
  border-radius: 10rpx;
  margin-right: 15rpx;
}
.carlist .listcenter .listcon .conright{
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  width: 456rpx;
}
.carlist .listcenter .listcon .conright .contit{
  font-size: 26rpx;
  color: #333;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  width: 100%;
  line-height: 38rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}
.carlist .listcenter .listcon .conright .conbottom{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: auto;
}
.carlist .listcenter .listcon .conright .conbottom .guige{
  color: #666;
  margin-bottom: 5rpx;
  font-size: 24rpx;
}
.carlist .listcenter .listcon .conright .conbottom .money{
  display: flex;
  align-items: center;
  color: #D32C44;
  font-size: 24rpx;
}
.carlist .listcenter .listcon .conright .conbottom .money .peisong{
  font-size: 19rpx;
  margin-left: 23rpx;
}
.carlist .listbottom{
  width: 100%;
  height: 110rpx;
  box-sizing: border-box;
  padding-left: 26rpx;
  padding-right: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.carlist .listbottom .left{
  display: flex;
  align-items: center;
  height: 110rpx;
}
.carlist .listbottom .left .heji{
  font-size: 24rpx;
  color: #333;
}
.carlist .listbottom .left .money{
  font-size: 32rpx;
  color: #D32C44;
}
.carlist .listbottom .right{
  width: 178rpx;  
  height: 70rpx;
  border-radius: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
  color: #fff;
  
  text-align: center;
  background: #ededed;
}
.carlist .listbottom .right.active{
  color: #fff;
  background: linear-gradient(to right, #F9B623 , #EF8321);
}
.carlist .pSmore{
  width: 100%;
  height: 49rpx;
  display: flex;
  align-items: center;
	background-color: #f7ffde; 
}
.carlist .pSmore .pSmorecont{
  height: 49rpx;
  margin-left: 109rpx;
  display: flex;
  align-items: center;
  font-size: 21rpx;
}
.carlist .pSmore .pSmorecont image{
  width: 28rpx;
	height: 28rpx;
}
.carlist .pSmore .pSmorecont text{
  margin: 0rpx 15rpx;
}

/* 猜你喜欢标题 */
.tuijiantitbox{
  width: 750rpx;  
  height: 88rpx; 
  position: relative;
  
  text-align: center;
  line-height: 80rpx;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}
.tuijiantitbox .xianbg{
  width: 750rpx;
  height: 88rpx;
  position: absolute;
  left: 0;
  top: 0;

}
.tuijiantitbox .xian{
  width: 50rpx;
  height: 2rpx;
  background: #333;
}
.tuijiantitbox>text{
  font-size: 34rpx; 
  color: #333;
  font-weight: 700;
  letter-spacing: 10rpx;
  margin-left: 20rpx;
  margin-right: 10rpx;

}
.tuijiantitbox .right{
  position: absolute;
  width: 120rpx;
  height: 100%;
  top: 0;
  right: 25rpx;
  z-index: 5;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.tuijiantitbox .right text{
  font-size: 24rpx;
  color: #0E9F62;
  
}
.tuijiantitbox .right image{
  width: 26rpx;
  height: 26rpx;
  margin-right: 5rpx;
}
/* 内容框 */
.listbox{
  width: 100%;
  box-sizing: border-box;
  padding: 0 16rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding-bottom: 20rpx;
}
.listbox .list{
  width: 350rpx;
  display: flex;
  flex-direction: column;
  border-radius: 10rpx;
  background: #fff;
  overflow: hidden;
  margin-top: 10rpx;
  box-sizing: border-box;
  padding-bottom: 15rpx;
}
.listbox .list image{
  width: 350rpx;
  height: 350rpx;
}
.listbox .list .listcon{
  width: 345rpx;
  height: 150rpx;
  box-sizing: border-box;
  padding: 0 15rpx;
  display: flex;
  flex-direction: column;
}
.listbox .list .listcon .tit{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-top: 10rpx;
  overflow: hidden;
  box-sizing: border-box;
  padding: 5rpx 0;
}
.listbox .list .listcon .tit .biaoqian{
  padding: 5rpx 10rpx;
  border: 1rpx solid #E9130D;
  border-radius: 10rpx;
  color: #E9130D;
  font-size: 22rpx;
  margin-right: 10rpx;
  transform: rotateZ(360deg);
}
.listbox .list .listcon .tit .name{
  color: #333;
  font-size: 24rpx;
  line-height: 1;
}
.listbox .list .listcon .bottom{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: auto;
}
.listbox .list .listcon .bottom .money text:nth-of-type(1){
  font-size: 24rpx;
  color: #E9130D;
}
.listbox .list .listcon .bottom .money text:nth-of-type(2){
  font-size: 32rpx;
  color: #E9130D;
}
.listbox .list .listcon .bottom .juli{
  font-size: 22rpx;
  color: #999;
}
/* 购物车数量改变样式 */
.spinner_r{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  width:160rpx;
height:44rpx;
border:1px solid rgba(216,216,216,1);
border-radius:10rpx;
overflow: hidden;
}
.spinner_r text{
  flex: 1;
  text-align: center;
  font-size: 26rpx;
}
.spinner_r input{
  flex: 1;
  text-align: center;
  border-left: 1px solid #eee;
   border-right: 1px solid #eee;
   color: #212121;
   font-size: 24rpx;
}
